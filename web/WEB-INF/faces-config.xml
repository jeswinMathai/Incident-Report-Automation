	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
<application>
	<resource-bundle>
		<base-name>com.service.application</base-name>
		<var>msg</var>
	</resource-bundle>
</application>



<flow-definition id="createReport">
